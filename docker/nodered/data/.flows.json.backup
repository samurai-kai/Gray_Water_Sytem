    {
        "id": "fn_mqtt_to_influx",
        "type": "function",
        "z": "f_graywater_flow",
        "name": "MQTT â†’ Influx line",
        "func": "// Topic format:\n//   graywater/clean/percent\n//   graywater/clean/gallons\n//   graywater/clean/water_out_diff\n//   graywater/dirty/percent\n//   graywater/dirty/gallons\n//   graywater/dirty/pump_state\n\nconst parts = msg.topic.split(\"/\");\nif (parts.length < 3) {\n    return null; // ignore old topics or malformed\n}\n\nconst tank = parts[1];     // \"clean\" or \"dirty\"\nconst field = parts[2];    // \"percent\", \"gallons\", \"cycles\", etc.\nconst value = parseFloat(msg.payload);\n\nif (isNaN(value)) return null;\n\n// Measurement:\nconst measurement = `${tank}_tank`;  // clean_tank or dirty_tank\n\n// Optional tag (if you want to differentiate devices)\nconst tags = `source=${tank}`;\n\n// Build Influx line protocol\nmsg.payload = `${measurement},${tags} ${field}=${value}`;\n\n// Build request to InfluxDB 2.x\nmsg.method = \"POST\";\nmsg.url = \"http://influxdb:8086/api/v2/write?org=graywater&bucket=graywater&precision=s\";\n\nmsg.headers = {\n    \"Authorization\": \"Token NsDIpIMdKJr1IESaIiRaF7pvbS58voCd1pDOxqNGZN2SXR2v9cay7QO0MQrgsj90RSYTT6n9Hy9S2QvSH1VezA==\",\n    \"Content-Type\": \"text/plain\"\n};\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 470,
        "y": 160,
        "wires": [
